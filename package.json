    "test": "minijasminenode test.js",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "keywords": [
    "function",
    "throttle",
    "invoke"
  ],
  "devDependencies": {
    "minijasminenode": "^1.1.1",
    "mocha": "*",
    "rollup": "^0.59.2",
    "should": "*",
    "sinon": "^1.17.7"
  },
  "component": {
    "scripts": {
      "debounce/index.js": "index.cjs.js"
    }
  }
